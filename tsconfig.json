{
  "compilerOptions": {
  "target": "ES2020",
  "module": "ESNext",
  "lib": ["ES2020", "DOM", "WebWorker"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "include": [
  "assets/ts/**/*",
  "assets/**/*.ts",
  "assets/**/*.tsx",
  "assets/ts/types/**/*"
  ]
}